query getQuote {
  getQuote(id: "8c51c279-d51b-4d0e-9988-efcedc2041e8"){
    id
    content
    author
    createdDate
    likes
    dislikes

  }
}

query allQuote{
  allQuote{
      items{
        id
        content
        author
        createdDate
        likes
        dislikes

    }
  }
}
mutation putQuote{
  putQuote(
		content: "hello world4",
		author: "steve jobs4",
		createdDate: "2018 1 14",
	){
  	id
    content
    author
    createdDate
    likes
    dislikes

  }
}

mutation incrementLikes{
  incrementQuoteLikes(id: "8c51c279-d51b-4d0e-9988-efcedc2041e8")
  {
  	id
    content
    author
    createdDate
    likes
    dislikes

  }
}

mutation incrementDislikes{
  incrementQuoteDislikes(id: "8c51c279-d51b-4d0e-9988-efcedc2041e8")
  {
  	id
    content
    author
    createdDate
    likes
    dislikes

  }
}